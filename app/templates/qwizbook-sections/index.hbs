<div class="container">
  <div class="row">
    <h2>Sections </h2>
    <hr/>


  {{#each section in model}}

  {{#if section.isStarting}}
  <div class="panel panel-primary">
  {{else}}
  <div class="panel panel-default">
  {{/if}}
    <div class="panel-heading clearfix">
      <div class="pull-right">
        <div class="btn-group" role="group">
          <button type="button" class="btn btn-default " {{ action "editSection" this }}><i class="fa fa-pencil fa-1x"></i></button>
        </div>
      </div>
      </div>
    <div class="panel-body">
      {{section.title }}
    </div>
  </div>
  {{/each}}
</div>

</div>
